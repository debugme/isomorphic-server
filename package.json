{
  "name": "isomorphic-server",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "clean:assets": "rm -rf node_modules build public && npm install",
    "build:client": "webpack --config ./webpack.client.babel.js --watch",
    "build:server": "webpack --config ./webpack.server.babel.js --watch",
    "start:server": "nodemon --watch build --exec \" node build/server.bundle.js \""
  },
  "license": "GNU GPL 3.0",
  "bugs": {
    "url": "https://github.com/debugme/isomorphic-server/issues"
  },
  "homepage": "https://github.com/debugme/isomorphic-server#readme",
  "dependencies": {
    "express": "4.16.2",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "webpack": "3.10.0",
    "nodemon": "1.12.5"
  }
}